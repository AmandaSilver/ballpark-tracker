{
    "name": "Ball Park Tracker",
    "appPort": 3000,
    "extensions": [
        "dbaeumer.vscode-eslint",
        "karigari.chat"
    ],
    "dockerComposeFile": "docker-compose.yml",
	"service": "node",
	"workspaceFolder": "/workspace",
    "remoteUser": "node",
    "settings": {
        "files.autoSave": "off"
    },
    "postCreateCommand": "bash .devcontainer/postCreate.sh"
}
