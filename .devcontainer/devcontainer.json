{
    "name": "Ball Park Tracker",
    "forwardPorts": [3000],
    "extensions": [
        "dbaeumer.vscode-eslint",
        "karigari.chat"
    ],
    "dockerComposeFile": "docker-compose.yml",
	"service": "node",
	"workspaceFolder": "/workspace",
    "remoteUser": "node",
    "settings": {
        "files.autoSave": "off",
	"terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "bash .devcontainer/postCreate.sh"
}
